e.json().then(function(pokemon){\n                document.getElementById(\"name\").innerHTML = pokemon.name;\n                console.log(pokemon.sprites.front_default);\n\n                let image = document.getElementById(\"imgPokemon\");\n                image.setAttribute(\"src\", pokemon.sprites.front_default);\n            })\n        })\n    }\n  }\n};\n</script>\n\n<style scoped>\n\n#body img{\n    width: 800px;\n    background: red;\n}\n\n#target{\n    text-align: center;\n}\n#divImgPoke{\n    height: 200px;\n    width: 200px;\n    border: 1px solid gray;\n    margin: 0px auto;\n    background-image: url(\"../assets/field.webp\");\n}\n\n#data{\n    margin-top: 50px;\n    text-align: center;\n}\n\n#data input, button{\n    margin: 0px auto;\n    display: block;\n}\n\n#idPokemon{\n    width: 200px;\n    height: 20px;\n    border: 1px solid gray;\n    border-radius: 3px;\n    padding: 3px;\n    margin-bottom: 20px;\n}\n\nbutton{\n    width: 100px;\n    height: 30px;\n    border: 1px solid #ffcb75;\n    border-radius: 5px;\n    background: #f3e082;\n    color: #001f47;\n    margin-top: 25px;\n}\n\nbutton:hover{\n    background: #ffcb75;\n    color: #001f47;\n}\n\np{\n    font-family: Arial, Helvetica, sans-serif;\n}\n</style>"]}]}